# Copy this file to terraform.tfvars and customize values

aws_region      = "us-east-1"
user_pool_name  = "jenkins-users"
client_name     = "jenkins-client"
domain_prefix   = "jenkins-auth-unique-name"  # Must be globally unique

# Callback URLs for your Jenkins instance
callback_urls = [
  "http://localhost/securityRealm/finishLogin",
  "http://localhost:8080/securityRealm/finishLogin",
  "https://your-domain.com/jenkins/securityRealm/finishLogin"
]

logout_urls = [
  "http://localhost",
  "http://localhost:8080",
  "https://your-domain.com/jenkins"
]

# Admin user configuration
create_admin_user   = true
admin_username      = "admin"
admin_email         = "admin@yourcompany.com"
admin_name          = "Jenkins Administrator"
admin_temp_password = "ChangeMe123!"

# Resource tags
tags = {
  Environment = "development"
  Project     = "jenkins-auth"
  ManagedBy   = "terraform"
  Owner       = "devops-team"
}